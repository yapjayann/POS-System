<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="750.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ch.makery.address.view.HomePageController">
   <children>
      <Label layoutX="203.0" layoutY="55.0" text="Chrome Dinosaur Game">
         <font>
            <Font name="Rockwell" size="31.0" />
         </font>
      </Label>
      <ImageView fitHeight="165.0" fitWidth="214.0" layoutX="299.0" layoutY="106.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../../img/dino.png" />
         </image>
      </ImageView>
      <ButtonBar layoutX="224.0" layoutY="294.0" prefHeight="32.0" prefWidth="302.0">
         <buttons>
            <Button fx:id="startGame" alignment="CENTER" contentDisplay="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#getRequestPlayerName" prefHeight="25.0" prefWidth="148.0" text="Start" textAlignment="CENTER" />
            <Button fx:id="howToPlay" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#getHowToPlay" prefHeight="25.0" prefWidth="148.0" text="How To Play" />
         </buttons>
      </ButtonBar>
   </children>
</AnchorPane>
